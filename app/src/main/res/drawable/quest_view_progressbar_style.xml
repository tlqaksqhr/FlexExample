<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="18dp" />
            <solid android:color="#fafafa" />
            <gradient
                android:type="linear"
                android:startColor="#17fafafa"
                android:centerColor="#42fbfbfb"
                android:endColor="#00fafafa" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="18dp" />
                <gradient
                    android:type="linear"
                    android:startColor="#634ac8"
                    android:endColor="#3c87d1"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="18dp" />
                <gradient
                    android:type="linear"
                    android:startColor="#634ac8"
                    android:endColor="#3c87d1"/>
            </shape>
        </clip>
    </item>
</layer-list>