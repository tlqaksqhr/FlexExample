<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >


    <item
        android:width="360dp"
        android:height="60dp"
        android:left="50dp">
        <shape>
            <solid android:color="#ffffff"/>
            <stroke android:width="1dp" android:color="#d6d6d6"/>
        </shape>
    </item>

    <item android:left="50dp"
            android:width="45dp" android:height="43dp"
            android:right="40dp">

        <rotate android:fromDegrees="45"
                android:pivotX="2"
                android:pivotY="0">
            <shape>
                <solid android:color="#ffffff"/>
                <stroke android:width="1dp" android:color="#d6d6d6"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="50dp"
        android:height="60dp"
        android:left="50dp">
        <shape>
            <solid android:color="#ffffff"/>
        </shape>
    </item>

</layer-list>